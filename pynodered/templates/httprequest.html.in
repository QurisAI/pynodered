<script type="text/x-red" data-template-name="%(name)s">
%(form)s
</script>

<script type="text/x-red" data-help-name="%(name)s">
%(description)s
</script>


<script type="text/javascript">

    RED.nodes.registerType('%(name)s',{
        category: '%(category)s',
        color:"%(color)s",
        inputs: %(inputs)s,
        inputLabels: "%(input_label)s",
        outputs: %(outputs)s,
        paletteLabel: "%(palette_label)s",
        outputLabels : function(index) {
            %(labels_text)s
        },
        button: %(button)s,
        defaults: %(defaults)s,
        icon: "%(icon)s.png",
        labelStyle: "%(label_style)s",
        align: "%(align)s",
        label: function() {
            var labels = %(label)s;
            if (labels == []) {
                return "%(title)s";
            } else {
                var s = "%(title)s";
                try { if (this.myname != "" && this.myname != undefined) s = this.myname;}
                catch (e) {}
                var i;
                if (labels.length > 0) {
                    s += " - ";
                }
                for (i = 0 ; i < labels.length; i++) {
                    s += ' ' + labels[i];
                }
                return s;
            }
        }
    });
</script>

